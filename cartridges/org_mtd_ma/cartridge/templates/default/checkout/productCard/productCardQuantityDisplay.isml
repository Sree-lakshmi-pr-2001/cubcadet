<p class="line-item-pricing-info row no-gutters">
    <span class="line-item-total-text qty-card-quantity-label <isif condition="${confirmation || pdict.currentStage === 'placeOrder'}">col-3 <iselseif condition="${!miniCart}">col-5<iselse>col-6</isif>">
        ${Resource.msg('label.qty','cart',null)}:
    </span>
    <span class="pricing qty-card-quantity-count <isif condition="${!miniCart}">col-7<iselse>col-6</isif>"><isprint value="${lineItem.quantity}" formatter="##" /></span>
</p>

                <address-mapping key1="Canada" key2="Northwest Territories"/>
            </address-mappings>
        </tax-jurisdiction>

        <tax-jurisdiction jurisdiction-id="nova-scotia">
            <display-name>Nova Scotia</display-name>
            <address-mappings>
                <address-mapping key1="Canada" key2="Nova Scotia"/>
            </address-mappings>
        </tax-jurisdiction>

        <tax-jurisdiction jurisdiction-id="nunavut">
            <display-name>Nunavut</display-name>
            <address-mappings>
                <address-mapping key1="Canada" key2="Nunavut"/>
            </address-mappings>
        </tax-jurisdiction>

        <tax-jurisdiction jurisdiction-id="ontario" default="true">
            <display-name>Ontario</display-name>
            <address-mappings>
                <address-mapping key1="Canada" key2="Ontario"/>

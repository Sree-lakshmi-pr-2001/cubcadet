<?xml version="1.0" encoding="UTF-8"?>
<tax xmlns="http://www.demandware.com/xml/impex/tax/2007-02-14">
    <tax-classes>
        <tax-class class-id="standard" default="true">
            <display-name>Standard</display-name>
            <description/>
        </tax-class>

    </tax-classes>
    <tax-jurisdictions>
        <tax-jurisdiction jurisdiction-id="Canada">
            <display-name>Saskatchewan</display-name>
            <address-mappings>
                <address-mapping key1="Canada" key2="Saskatchewan"/>
            </address-mappings>
        </tax-jurisdiction>

        <tax-jurisdiction jurisdiction-id="alberta">
            <display-name>Alberta</display-name>
            <address-mappings>
                <address-mapping key1="Canada" key2="Alberta"/>
            </address-mappings>
        </tax-jurisdiction>

        <tax-jurisdiction jurisdiction-id="british-columbia">
            <display-name>British Columbia</display-name>
            <address-mappings>
                <address-mapping key1="Canada" key2="British Columbia"/>
            </address-mappings>
        </tax-jurisdiction>

        <tax-jurisdiction jurisdiction-id="manitoba">
            <display-name>Manitoba</display-name>
            <address-mappings>
                <address-mapping key1="Canada" key2="Manitoba"/>
            </address-mappings>
        </tax-jurisdiction>

        <tax-jurisdiction jurisdiction-id="new-brunswick">
            <display-name>New Brunswick</display-name>
            <address-mappings>
                <address-mapping key1="Canada" key2="New Brunswick"/>

        </tax-jurisdiction>

        <tax-jurisdiction jurisdiction-id="prince-edward-island">
            <display-name>Prince Edward Island</display-name>
            <address-mappings>
                <address-mapping key1="Canada" key2="Prince Edward Island"/>
            </address-mappings>
        </tax-jurisdiction>

        <tax-jurisdiction jurisdiction-id="quebec">
            <display-name>Quebec</display-name>
            <address-mappings>
                <address-mapping key1="Canada" key2="Quebec"/>
            </address-mappings>
        </tax-jurisdiction>

        <tax-jurisdiction jurisdiction-id="yukon">
            <display-name>Yukon</display-name>
            <address-mappings>
                <address-mapping key1="Canada" key2="Yukon"/>
            </address-mappings>
        </tax-jurisdiction>

    </tax-jurisdictions>
    <tax-rates>
        <tax-rate jurisdiction-id="yukon" class-id="standard">0.05</tax-rate>

        <tax-rate jurisdiction-id="nunavut" class-id="standard">0.05</tax-rate>

        <tax-rate jurisdiction-id="manitoba" class-id="standard">0.12</tax-rate>

        <tax-rate jurisdiction-id="quebec" class-id="standard">0.14975</tax-rate>

        <tax-rate jurisdiction-id="new-brunswick" class-id="standard">0.15</tax-rate>

        <tax-rate jurisdiction-id="nova-scotia" class-id="standard">0.15</tax-rate>

        <tax-rate jurisdiction-id="newfoundland" class-id="standard">0.15</tax-rate>

        <tax-rate jurisdiction-id="alberta" class-id="standard">0.05</tax-rate>

        <tax-rate jurisdiction-id="ontario" class-id="standard">0.13</tax-rate>

        <tax-rate jurisdiction-id="british-columbia" class-id="standard">0.12</tax-rate>

        <tax-rate jurisdiction-id="Canada" class-id="standard">0.11</tax-rate>

        <tax-rate jurisdiction-id="northwest-territories" class-id="standard">0.05</tax-rate>

        <tax-rate jurisdiction-id="prince-edward-island" class-id="standard">0.15</tax-rate>

    </tax-rates>

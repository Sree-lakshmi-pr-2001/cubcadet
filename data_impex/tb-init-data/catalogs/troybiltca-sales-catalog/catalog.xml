<?xml version="1.0" encoding="UTF-8"?>
<catalog xmlns="http://www.demandware.com/xml/impex/catalog/2006-10-31" catalog-id="troybiltca-sales-catalog">
    <header>
        <image-settings>
            <internal-location base-path="/"/>
            <view-types>
                <view-type>large</view-type>
                <view-type>swatch</view-type>
                <view-type>360view</view-type>
            </view-types>
            <variation-attribute-id>style</variation-attribute-id>
            <alt-pattern>${productname}</alt-pattern>
            <title-pattern>${productname}</title-pattern>
        </image-settings>
    </header>

    <category category-id="root">
        <display-name xml:lang="x-default">Troy-Bilt CA Storefront Catalog</display-name>
        <online-flag>true</online-flag>
        <template/>
        <page-attributes/>
    </category>

</catalog>

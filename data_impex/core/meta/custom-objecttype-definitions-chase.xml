        <staging-mode>no-staging</staging-mode>
        <storage-scope>organization</storage-scope>
        <key-definition attribute-id="serverID">
            <display-name xml:lang="x-default">Server ID</display-name>
            <type>string</type>
            <min-length>0</min-length>
        </key-definition>
        <attribute-definitions>
            <attribute-definition attribute-id="failedTime">
                <display-name xml:lang="x-default">Failed time</display-name>
                <type>datetime</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="isFailed">
                <display-name xml:lang="x-default">Is Failed</display-name>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="serverURL">
                <display-name xml:lang="x-default">Server URL</display-name>
                <type>string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
        </attribute-definitions>
        <group-definitions>
            <attribute-group group-id="General">
                <display-name xml:lang="x-default">General</display-name>
                <attribute attribute-id="serverID"/>
                <attribute attribute-id="serverURL"/>
                <attribute attribute-id="isFailed"/>
                <attribute attribute-id="failedTime"/>
                <attribute attribute-id="lastModified" system="true"/>
            </attribute-group>
        </group-definitions>
    </custom-type>
</metadata>

    <type-extension type-id="Category">
        <custom-attribute-definitions>
             <attribute-definition attribute-id="mtdWebToLeadID">
                <display-name xml:lang="x-default">Web to Lead Form ID</display-name>
                <description xml:lang="x-default">The content asset ID of a web to lead form asset that should display for this category.</description>
                <type>string</type>
            </attribute-definition>
            <attribute-definition attribute-id="alternativeUrl">
                <display-name xml:lang="x-default">Alternative URL</display-name>
                <description xml:lang="x-default">Renders an alternative URL in main navigation. Uses Commerce Cloud content url notation. For example: $url('Account-Show')$ or normal URL http://xchange.demandware.com</description>
                <type>html</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <field-length>0</field-length>
                <field-height>2</field-height>
            </attribute-definition>
            <attribute-definition attribute-id="catBannerID">
                <display-name xml:lang="x-default">Category Banner ID</display-name>
                <description xml:lang="x-default">Used to define the content asset used to populate a grid page banner for a category. This value is applied to all sub-category navigation (cascading) if no specific catBannerID has been defined for  a sub-category.</description>
                <type>string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="enableCompare">
                <display-name xml:lang="x-default">Enable Compare</display-name>
                <description xml:lang="x-default">Used to define if/when the Compare feature is to be visualized in the storefront based on navigation. If enableCompare = FALSE, no Compare checkboxes will be displayed in the grid view. If enableCompare = TRUE, the category (and its children) will support the Compare feature.</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="headerMenuBanner">
                <display-name xml:lang="x-default">Header Menu Banner</display-name>
                <type>html</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="headerMenuOrientation">
                <display-name xml:lang="x-default">Header Menu Orientation</display-name>
                <description xml:lang="x-default">Which way to orient the menu and optional header menu HTML. Vertical will list all in one line. Horizontal will list in columns.</description>
                <type>enum-of-string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <value-definitions>
                    <value-definition default="true">
                        <display xml:lang="x-default">Horizontal</display>
                        <value>Horizontal</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">Vertical</display>
                        <value>Vertical</value>
                    </value-definition>
                </value-definitions>
            </attribute-definition>
            <attribute-definition attribute-id="showInMenu">
                <display-name xml:lang="x-default">Show in Menu Navigation</display-name>
                <description xml:lang="x-default">Used to indicate that a category (such as Mens -&gt; Footwear -&gt; Boots) will display in the roll-over navigation. A sub-category only shows if also the parent category is marked as showInMenu. Up to three category levels are shown in roll-over navigation.</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="sizeChartID">
                <display-name xml:lang="x-default">Size Chart</display-name>
                <description xml:lang="x-default">Used to define the content asset ID of the Size Chart that is appropriate for products whose PRIMARY category is the associated category (and its children). Whenever a product detail page (or quick view) is rendered, the Size Chart link is populated based on the value of this attribute for the products primary categorization. If not defined, NO size chart link is displayed.</description>
                <type>string</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="slotBannerImageMobile">
                <display-name xml:lang="x-default">Category Landing Slot Mobile Image</display-name>
                <description xml:lang="x-default"> Image displayed at the top of the Category Landing or Browse pages in mobile viewports. </description>
                <type>image</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="slotBannerImage">
                <display-name xml:lang="x-default">Category Landing Slot Desktop Image</display-name>
                <description xml:lang="x-default"> Image displayed at the top of the Category Landing or Browse pages in desktop viewports. </description>
                <type>image</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="seoText">
                <display-name xml:lang="x-default">SEO HTML</display-name>
                <description xml:lang="x-default">SEO copy displayed at the top of the Category Browse pages.</description>
                <type>html</type>
                <localizable-flag>true</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
        </custom-attribute-definitions>
        <group-definitions>
            <attribute-group group-id="customAttributes">
                <display-name xml:lang="x-default">Custom</display-name>
                <attribute attribute-id="sizeChartID"/>
                <attribute attribute-id="showInMenu"/>
                <attribute attribute-id="enableCompare"/>
                <attribute attribute-id="catBannerID"/>
                <attribute attribute-id="headerMenuOrientation"/>
                <attribute attribute-id="headerMenuBanner"/>
                <attribute attribute-id="alternativeUrl"/>
                <attribute attribute-id="slotBannerImageMobile"/>
                <attribute attribute-id="slotBannerImage"/>

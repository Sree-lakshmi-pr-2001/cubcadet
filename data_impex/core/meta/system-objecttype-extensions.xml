            </attribute-group>
            <attribute-group group-id="MTD">
                <display-name xml:lang="x-default">MTD</display-name>
                <attribute attribute-id="mtdWebToLeadID"/>
            </attribute-group>
        </group-definitions>
    </type-extension>

    <content content-id="td-estimated-payment-cart-apr">
        <display-name xml:lang="x-default">Cub Cadet TD Estimated Payment PDP APR</display-name>
        <online-flag>true</online-flag>
        <searchable-flag>false</searchable-flag>
        <page-attributes/>
        <custom-attributes>
        <custom-attribute attribute-id="body" xml:lang="x-default">&lt;div class=&quot;td-details&quot;&gt;&#13;&lt;h3&gt;Work Now, Pay Later
&lt;/h3&gt;&#13;&lt;div class=&quot;td-info&quot;&gt;&#13;
Your Order is eligible for Cub Cadet Financing.&#13;
&lt;/div&gt;&#13;&lt;div class=&quot;td-info-detail&quot;&gt;&#13;
&lt;strong&gt;${monthlypayment} Per Month, &lt;/strong&gt;&lt;strong&gt;{termlength} months&lt;/strong&gt; &lt;strong&gt;{apr}% APR&lt;sup&gt;{superscriptnumber}&lt;/sup&gt; &lt;/strong&gt;financing with &lt;strong&gt;${processfee} Promo Fee&lt;/strong&gt;. Total: ${totalpayment}&#13;&lt;/div&gt;&#13;&lt;/div&gt;&#13;
&lt;div class=&quot;td-buttons&quot;&gt;&#13;&lt;div class=&quot;td-apply-button&quot;&gt;&#13;
&lt;a href=&quot;/&quot; data-url=&quot;$url('FinancingApplication-GetURL')$&quot; class=&quot;td-apply-finance btn btn-outline-secondary&quot;&gt;Apply Now&lt;/a&gt;&#13;&lt;/div&gt;&#13;&lt;div class=&quot;td-payment&quot;&gt;&#13;
&lt;a href=&quot;/&quot; data-url=&quot;$url('FinancingApplication-Disclosure')$&quot; data-title=&quot;Details &amp; Disclosures&quot; class=&quot;td-payment-details&quot;&gt;See Details&lt;/a&gt;&#13;&lt;/div&gt;&#13;&lt;/div&gt;</custom-attribute>
        </custom-attributes>
    </content>


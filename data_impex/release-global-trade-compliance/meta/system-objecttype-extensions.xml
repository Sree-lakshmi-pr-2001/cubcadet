<?xml version="1.0" encoding="UTF-8"?>
<metadata xmlns="http://www.demandware.com/xml/impex/metadata/2006-10-31">

    <type-extension type-id="Order">
        <custom-attribute-definitions>
            <attribute-definition attribute-id="globalComplianceCheckStatus">
                <display-name xml:lang="x-default">globalComplianceCheckStatus</display-name>
                <type>enum-of-string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <value-definitions>
                    <value-definition default="true">
                        <display xml:lang="x-default">PENDING</display>
                        <value>PENDING</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">MANUAL</display>
                        <value>MANUAL</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">APPROVED</display>
                        <value>APPROVED</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">NOT_APPROVED</display>
                        <value>NOT_APPROVED</value>
                    </value-definition>
                </value-definitions>
            </attribute-definition>
        </custom-attribute-definitions>
        <group-definitions>
            <attribute-group group-id="General">
                <display-name xml:lang="x-default">General</display-name>
                <attribute attribute-id="prop65Acknowledged"/>
                <attribute attribute-id="addressValidated"/>
                <attribute attribute-id="globalComplianceCheckStatus"/>
            </attribute-group>
        </group-definitions>
    </type-extension>

    <type-extension type-id="SitePreferences">

    <content content-id="model-number-modal">
        <display-name xml:lang="x-default">How to Find Modal Number</display-name>
        <online-flag>true</online-flag>
        <searchable-flag>false</searchable-flag>
        <custom-attributes>
            <custom-attribute attribute-id="body" xml:lang="x-default">&lt;p&gt;&lt;span class=&quot;img-wrap&quot;&gt; &lt;img alt=&quot;model plate example with model number in the top left corner outlined in red and serial number in top right corner outlined in red&quot; src=&quot;images/model-number-label.png?$staticlink$&quot; title=&quot;&quot; /&gt; &lt;/span&gt;&lt;/p&gt;&#13;&lt;p&gt;The model number is&lt;/p&gt;&#13;&lt;ul&gt;&#13;	&lt;li&gt;11 characters long&lt;/li&gt;&#13;	&lt;li&gt;Printed on the left side of the Product Identification Label&lt;/li&gt;&#13;&lt;/ul&gt;&#13;&lt;p&gt;Characters can include numbers, letters or hyphens.&lt;/p&gt;&#13;&lt;style type=&quot;text/css&quot;&gt;.model-num-modal h4 {&#13;          margin-bottom: 1rem;&#13;        }&#13;        .model-num-modal .modal-header {&#13;          padding-bottom: 0;&#13;        }&#13;        .model-num-modal .modal-body {&#13;          clear: both;&#13;          overflow: hidden;&#13;        }&#13;        .model-num-modal .img-wrap {&#13;          width: 300px;&#13;          display: block;&#13;          float:right;&#13;          margin-bottom: 1rem;&#13;        }&#13;        .model-num-modal ul {&#13;          padding-left: 1rem;&#13;        }&#13;        .model-num-modal img {&#13;          max-width: 100%;&#13;        }&#13;&lt;/style&gt;</custom-attribute>
        </custom-attributes>
        <folder-links>
        </folder-links>
    </content>


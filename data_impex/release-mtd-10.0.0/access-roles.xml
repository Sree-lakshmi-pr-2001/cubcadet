        <access-controls>
            <access-control resource-path="BUSINESSMGR/CustomMenu/Sites/-/customsubmenu_map_manager" permission="ACCESS"/>
            <access-control resource-path="BUSINESSMGR/CustomMenu/Sites/-/customsubmenu_tools_bmsession" permission="ACCESS"/>
            <access-control resource-path="BUSINESSMGR/CustomMenu/Sites/-/customsubmenu_tools_fsBrowser" permission="ACCESS"/>
            <access-control resource-path="BUSINESSMGR/CustomMenu/Sites/-/customsubmenu_tools_httpBrowser" permission="ACCESS"/>
            <access-control resource-path="BUSINESSMGR/CustomMenu/Sites/-/customsubmenu_tools_terminal" permission="ACCESS"/>
		</access-controls>
    </access-role>
</access-roles>
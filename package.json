{
  "name": "lyonscg-sfra",
  "version": "3.2.0",
  "lcgversion": "1.0.0",
  "description": "LyonsCG maintained version of SFCC SFRA",
  "engines": {
    "node": "8.9.0"
  },
  "scripts": {
    "test": "node build_tools/build --test ./test/unit/**/*.js",
    "test:appium": "node bin/Makefile functional -- appium -- ",
    "cover": "node build_tools/build --cover",
    "test:integration": "node bin/Makefile integration -- ",
    "test:functional": "node bin/Makefile functional -- ",
    "test:functional:docker": "bin/test-functional-docker.sh",
    "test:functional:sauce": "./node_modules/.bin/wdio test/functional/webdriver/wdio.conf.js ",
    "compile:js:prod": "webpack --config build_tools/tasks/compile.js --mode=production --config-name js",
    "compile:scss:prod": "webpack --config build_tools/tasks/compile.js --mode=production --config-name scss",
    "compile:js": "webpack --config build_tools/tasks/compile.js --mode=development --config-name js",
    "compile:scss": "webpack --config build_tools/tasks/compile.js --mode=development --config-name scss",
    "compile:fonts": "node bin/Makefile compileFonts",
    "watch": "webpack --config build_tools/tasks/compile.js --mode=development --watch --info-verbosity none",
    "lint": "node build_tools/build --lint",
    "upload": "node build_tools/build --upload",
    "uploadCartridge": "node build_tools/build --uploadCartridge",
    "release": "node bin/Makefile release --",
    "deployCartridges": "node build_tools/build --deployCartridges",
    "createCartridge": "node build_tools/build --createCartridge",
    "deployData:blog-mtd": "node build_tools/build --deploy-data --data-bundle=blog-mtd",
    "deploy": "node --max-old-space-size=3048 build_tools/build --deploy-data",
    "mtd_test": "node mtd_build_tools/build --test ./test/unit/**/*.js",
    "mtd_test:appium": "node bin/Makefile functional -- appium -- ",
    "mtd_cover": "node mtd_build_tools/build --cover",
    "mtd_compile:js:prod": "webpack --config mtd_build_tools/tasks/compile.js --mode=production --config-name js",
    "mtd_compile:scss:prod": "webpack --config mtd_build_tools/tasks/compile.js --mode=production --config-name scss",
    "mtd_compile:js": "webpack --config mtd_build_tools/tasks/compile.js --mode=development --config-name js",
    "mtd_compile:scss": "webpack --config mtd_build_tools/tasks/compile.js --mode=development --config-name scss",
    "mtd_compile:fonts": "node bin/Makefile compileFonts",
    "mtd_watch": "webpack --config mtd_build_tools/tasks/compile.js --mode=development --watch --info-verbosity none",
    "mtd_lint": "node mtd_build_tools/build --lint",
    "mtd_upload": "node mtd_build_tools/build --upload",
    "mtd_uploadCartridge": "node mtd_build_tools/build --uploadCartridge",
    "mtd_deployCartridges": "node mtd_build_tools/build --deployCartridges",
    "mtd_createCartridge": "node mtd_build_tools/build --createCartridge",
    "mtd_deploy": "node --max-old-space-size=3048 mtd_build_tools/build --deploy-data"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint -- --lint-no-cache"
